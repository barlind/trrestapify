{
  "name": "restapify",
  "version": "2.3.4",
  "license": "MIT",
  "private": false,
  "type": "module",
  "keywords": [
    "rest-api",
    "api",
    "mock",
    "json"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist",
    "bin"
  ],
  "bin": {
    "restapify": "bin/restapify"
  },
  "scripts": {
    "readme:generate-doc-table": "yarn doctoc ./docs/README.md ./docs/deployment/README.md --gitlab",
    "clean": "yarn rimraf ./dist",
    "start": "yarn nodemon --watch test src/index.js",
    "lint": "yarn eslint . --ext .js,.ts",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "rollup -c",
    "build": "yarn clean && yarn build:types && yarn build:js"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@babel/runtime": "^7.26.9",
    "@rollup/plugin-typescript": "^12.1.2",
    "boxen": "^8.0.1",
    "chalk": "^5.4.1",
    "chokidar": "^4.0.3",
    "commander": "^13.1.0",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "jsonschema": "^1.5.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "jwt-decode": "^4.0.0",
    "lodash.range": "^3.2.0",
    "nocache": "^4.0.0",
    "open": "^10.1.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.26.0",
    "@parcel/packager-ts": "2.13.3",
    "@parcel/transformer-typescript-types": "2.13.3",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/express-serve-static-core": "^5.0.6",
    "@types/lodash.range": "^3.2.9",
    "@types/node": "^22.13.4",
    "@types/serve-static": "^1.15.7",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "doctoc": "^2.2.1",
    "eslint": "^9.20.1",
    "fs-extra": "^11.3.0",
    "isomorphic-fetch": "^3.0.0",
    "nodemon": "^3.1.9",
    "replace-buffer": "^1.2.1",
    "rimraf": "^6.0.1",
    "rollup": "^4.34.8",
    "ts-loader": "^9.5.2",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "webpack": "^5.98.0"
  }
}
